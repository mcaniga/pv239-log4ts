    <string name="app_name">Aqi Check</string>
    <string name="general_error">Something went wrong :(</string>
    <string name="main_navigation_favorites">Favorites</string>
    <string name="main_navigation_list">List</string>
    <style name="AqiCard" parent="Widget.MaterialComponents.CardView">
        <item name="cardCornerRadius">12dp</item>
        <item name="strokeColor">@color/primary_variant_200</item>
        <item name="strokeWidth">1dp</item>
    </style>
    <style name="AqiSelectableChip" parent="Widget.MaterialComponents.Chip.Choice">
        <item name="chipBackgroundColor">@color/color_chip_selectable</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="TextMedium" parent="Widget.MaterialComponents.TextView">
        <item name="fontFamily">sans-serif-medium</item>
    </style>
    <style name="TextMedium.Large">
        <item name="android:textSize">16sp</item>
    </style>
    <style name="TextMedium.Large.Light">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="TextRegular" parent="Widget.MaterialComponents.TextView"/>
    <style name="TextRegular.Large">
        <item name="android:textSize">16sp</item>
    </style>
    <style name="TextRegular.Normal">
        <item name="android:textSize">14sp</item>
    </style>
    <style name="TextRegular.Normal.Light">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="Theme.AqiCheck" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" ns1:targetApi="l">@color/primary_variant_500</item>
        <!-- Customize your theme here. -->
        <item name="android:background">@color/primary_variant_500</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
</resources>